{
  "processors": [[ "stylelint-processor-styled-components", {
   "parserPlugins": [
      "jsx",
      "objectRestSpread",
      ["decorators", { "decoratorsBeforeExport": true }],
      "classProperties",
      "exportExtensions",
      "asyncGenerators",
      "functionBind",
      "functionSent",
      "dynamicImport",
      "optionalCatchBinding",
      "optionalChaining",
      "nullishCoalescingOperator"
    ]
   }]],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components"
  ],
}
